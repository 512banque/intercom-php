{
    "name": "Intercom API (User) v3",
    "description": "The Intercom API (User) v3",
    "operations": {
        "getUserByID": {
            "httpMethod": "GET",
            "parameters": {
                "id": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "UserModel",
            "responseType": "model",
            "summary": "Gets a single user by their ID",
            "uri": "/users/{id}"
        },
        "getUserByEmail": {
            "httpMethod": "GET",
            "parameters": {
                "email": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "UserModel",
            "responseType": "model",
            "summary": "Gets a single user by their email",
            "uri": "/users?email={email}"
        },
        "getUserByUserID": {
            "httpMethod": "GET",
            "parameters": {
                "user_id": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "UserModel",
            "responseType": "model",
            "summary": "Gets a single user by user id",
            "uri": "/users?user_id=user_id"
        },
        "getUsers": {
            "extends": "_abstract_pagination_page",
            "httpMethod": "POST",
            "responseClass": "UserListModel",
            "responseType": "model",
            "summary": "Gets a list of users",
            "uri": "/users/list"
        },
        "createUser": {
            "httpMethod": "POST",
            "parameters": {
                "companies": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "created_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "custom_attributes": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "email": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "last_request_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "last_seen_ip": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "last_seen_user_agent": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "name": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "unsubscribed_from_emails": {
                    "location": "json",
                    "required": false,
                    "type": "boolean"
                },
                "user_id": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                }
            },
            "responseClass": "UserModel",
            "responseType": "model",
            "summary": "Saves a user",
            "uri": "/users"
        },
        "updateUser": {
            "httpMethod": "POST",
            "parameters": {
                "id": {
                    "location": "json",
                    "required": true,
                    "type": "string"
                },
                "companies": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "created_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "custom_attributes": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "email": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "last_request_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "last_seen_ip": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "last_seen_user_agent": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "name": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "unsubscribed_from_emails": {
                    "location": "json",
                    "required": false,
                    "type": "boolean"
                },
                "update_last_request_at": {
                    "location": "json",
                    "required": false,
                    "type": "boolean"
                },
                "user_id": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                }
            },
            "responseClass": "UserModel",
            "responseType": "model",
            "summary": "Updates the user",
            "uri": "/users"
        },
        "deleteUser": {
            "httpMethod": "DELETE",
            "parameters": {
                "id": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "email": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "user_id": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                }
            },
            "responseType": "string",
            "summary": "Deletes a user given id, email or user_id",
            "uri": "/users"
        }
