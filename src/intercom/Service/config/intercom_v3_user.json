{
    "name": "Intercom API (User) v3",
    "description": "The Intercom API (User) v3",
    "operations": {
        "getUserByID": {
            "httpMethod": "GET",
            "parameters": {
                "id": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "UserModel",
            "responseType": "model",
            "summary": "Gets a single user by their ID",
            "uri": "/users/{id}"
        },
        "getUsers": {
            "httpMethod": "POST",
            "responseClass": "UserListModel",
            "responseType": "model",
            "summary": "Gets a list of users",
            "uri": "/users/list"
        },
        "getsUsersByFilters": {
            "httpMethod": "POST",
            "parameters": {
                "filters": {
                    "description": "An array of filter objects",
                    "location": "json",
                    "required": true,
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "attribute": {
                                "description": "The field to compare against",
                                "required": true,
                                "type": "string"
                            },
                            "comparison": {
                                "description": "The comparison operator - valid values are: lt gt eq starts_with, ends_with, contains, not_contains, known, unknown",
                                "required": true,
                                "type": "string"
                             },
                            "value": {
                                "description": "The filter value for the comparison",
                                "required": true,
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "responseClass": "UserListModel",
            "responseType": "model",
            "summary": "Gets a list of users that match the specified filter",
            "uri": "/users/list"
        },
        "SaveUser": {
            "httpMethod": "POST",
            "parameters": {
                "companies": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "created_at": {
                    "location": "json",
                    "required": false,
                    "type": "numeric",
                    "format": "timestamp"
                },
                "custom_data": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "email": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "increments": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "last_request_at": {
                    "location": "json",
                    "required": false,
                    "type": "numeric",
                    "format": "timestamp"
                },
                "last_seen_ip": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "last_seen_user_agent": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "name": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "unsubscribed_from_emails": {
                    "location": "json",
                    "required": false,
                    "type": "boolean"
                },
                "user_id": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                }
            },
            "responseClass": "UserModel",
            "responseType": "model",
            "summary": "Saves a user",
            "uri": "/users"
        }
    },
    "models": {
