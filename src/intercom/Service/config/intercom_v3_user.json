                            "type" : "string"
                        },
                        "region_code": {
                            "type" : "string"
                        },
                        "region_name": {
                            "type" : "string"
                        },
                        "timezone": {
                            "type" : "string"
                        }
                    }
                },
                "id": {
                    "type": "string"
                },
                "last_contacted_at": {
