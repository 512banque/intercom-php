    "models": {
        "UserModel": {
            "type": "object",
            "properties": {
                "app_id": {
                    "location": "json",
                    "type": "string"
                },
                "avatar": {
                    "location": "json",
                    "type": "object",
                    "items": {
                        "image_url": {
                            "type": "string"
                        }
                    }
                },
                "companies": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "companies": {
                            "location": "json",
                            "type": "array",
                            "items": {
                                "$ref": "CompanyModel"
                            }
                        }
                    }
                },
                "created_at": {
                    "location": "json",
                    "type": "integer"
                },
                "custom_attributes": {
                    "location": "json",
                    "type": "object"
                },
                "email": {
                    "location": "json",
                    "type": "string"
                },
                "location_data": {
                    "location": "json",
                    "type": "object",
                    "items": {
                        "city_name": {
                            "type": "string"
                        },
                        "continent_code": {
                            "type": "string"
                        },
                        "country_code": {
                            "type": "string"
                        },
                        "latitude": {
                            "type": "integer"
                        },
                        "longitude": {
                            "type": "integer"
                        },
                        "postal_code": {
                            "type": "string"
                        },
                        "region_name": {
                            "type": "string"
                        },
                        "timezone": {
                            "type": "string"
                        }
                    }
                },
                "id": {
                    "location": "json",
                    "type": "string"
                },
                "last_request_at": {
                    "location": "json",
                    "type": "integer"
                },
                "name": {
                    "location": "json",
                    "type": "string"
                },
                "remote_created_at": {
                    "location": "json",
                    "type": "integer"
                },
                "session_count": {
                    "location": "json",
                    "type": "integer"
                },
                "social_profiles": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "social_profiles": {
                            "location": "json",
                            "type": "array",
                            "items": {
                                "location": "json",
                                "type": "object",
                                "properties": {
                                    "type": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "id": {
                                        "type": "string"
                                    },
                                    "username": {
                                        "type": "string"
                                    },
                                    "url": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "segments": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "segments": {
                            "type": "array",
                            "location": "json",
                            "items": {
                                "$ref": "SegmentModel"
                            }
                        }
                    }
                },
                "tags": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "tags": {
                            "type": "array",
                            "location": "json",
                            "items": {
                                "$ref": "TagModel"
                            }
                        }
                    }
                },
                "unsubscribed_from_emails": {
                    "location": "json",
                    "type": "boolean"
                },
                "updated_at": {
                    "location": "json",
                    "type": "integer"
                },
                "user_agent_data": {
                    "location": "json",
                    "type": "string"
                }
            }
        },
        "UserListModel": {
            "type": "object",
            "properties": {
                "users": {
                    "type": "array",
                    "location": "json",
                    "items": {
                        "$ref": "UserModel"
                    }
                }
            }
        }
    }
}
