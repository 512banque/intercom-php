                                },
                                "normal_x2": {
                                    "type": "string"
                                }
                            }
                        },
                        "initials": {
                            "type": "string"
                        }
                    }
                },
                "companies": {
                    "type": "array",
                    "items": {
                        "$ref": "CompanyModel"
                    }
                },
                "company_ids": {
                    "type": "array",
