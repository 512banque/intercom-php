                    "required": false,
                    "type": "number"
                },
                "plan": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "session_count": {
                    "location": "json",
                    "required": false,
                    "type": "integer"
                },
                "custom_attributes": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                }
            },
