                    "type": "integer",
                    "format": "timestamp"
                },
                "monthly_spend": {
                    "location": "json",
                    "type": "number"
                },
                "name": {
                    "location": "json",
                    "type": "string"
                },
                "plan": {
                    "type": "object",
                    "items": {
                        "type": {
                            "location": "json",
                            "type": "string"
