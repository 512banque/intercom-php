            "extends": "_abstract_pagination_page",
            "httpMethod": "GET",
            "responseClass": "CompanyListModel",
            "responseType": "model",
            "summary": "Gets a list of companies",
            "uri": "/companies"
        },
        "getCompanyByID": {
            "httpMethod": "GET",
            "parameters": {
                "id": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "CompanyModel",
            "responseType": "model",
            "summary": "Gets a company by ID",
            "uri": "/companies/{id}"
        },
        "getCompanyByName": {
            "httpMethod": "GET",
            "parameters": {
                "name": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "CompanyModel",
            "responseType": "model",
            "summary": "Gets a company by name",
            "uri": "/companies?name={name}"
        },
        "getCompanyByCompanyID": {
            "httpMethod": "GET",
            "parameters": {
                "company_id": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "CompanyModel",
            "responseType": "model",
            "summary": "Gets a company by company_id parameter",
            "uri": "/companies?company_id={company_id}"
        },
        "getCompanyUsers": {
            "extends": "_abstract_pagination_page",
            "httpMethod": "GET",
            "parameters": {
                "id": {
                    "location": "uri",
