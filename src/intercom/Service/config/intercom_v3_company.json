{
    "name": "Intercom API (Company) v3",
    "description": "The Intercom API (Company) v3",
    "operations": {
        "_paginated_method": {
            "parameters": {
                "page": {
                    "location": "query",
                    "required": false,
                    "type": "integer"
                },
                "per_page": {
                    "location": "query",
                    "maximum": 60,
                    "required": false,
                    "type": "integer"
                }
            }
        },
        "getCompanies": {
            "extends": "_paginated_method",
            "httpMethod": "GET",
            "responseClass": "CompanyListModel",
            "responseType": "model",
            "summary": "Gets a list of companies",
            "uri": "/companies"
        },
        "getCompanyByID": {
            "httpMethod": "GET",
            "parameters": {
                "id": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "CompanyModel",
            "responseType": "model",
            "summary": "Gets a company by ID",
            "uri": "/companies/{id}"
        }
    },
    "models": {
        "CompanyModel": {
            "type": "object",
            "location": "json",
            "properties": {
                "app_id": {
                    "type": "string"
                },
                "created_at": {
                    "type": "integer"
                },
                "custom_data": {
                    "type": "object"
                },
                "id": {
                    "type": "string"
                },
                "last_request_at": {
                    "type": "integer"
                },
                "monthly_spend": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "plan_id": {
                    "type": "integer"
                },
                "remote_company_id": {
                    "type": "string"
                },
                "remote_created_at": {
                    "type": "integer"
                },
                "session_count": {
                    "type": "integer"
                },
                "tag_ids": {
                    "type": "string"
                },
                "updated_at": {
                    "type": "integer"
                },
                "user_count": {
                    "type": "integer"
                }
            }
        },
        "CompanyListModel": {
            "type": "object",
            "properties": {
                "companies": {
                    "type": "array",
                    "location": "json",
                    "items": {
                        "$ref": "CompanyModel"
                    }
                }
            }
        }
    }
}