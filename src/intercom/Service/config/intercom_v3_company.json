            "parameters": {
                "tag_id": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "CompanyListModel",
            "responseType": "model",
            "summary": "Gets all companies with given tag_id",
            "uri": "/companies?tag_id={tag_id}"
        },
        "getCompaniesBySegmentID": {
            "extends": "_abstract_pagination_page",
            "httpMethod": "GET",
            "parameters": {
                "segment_id": {
                    "location": "uri",
